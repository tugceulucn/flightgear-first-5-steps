<?xml version='1.0' encoding='utf-8'?>
<PropertyList>
    <sim>
        <!--Talking about aircraft on Flightgear GUI-->
        <description>Mandalina 1.0| IDEALAB (2024)</description>
        <author>Edip Sevincer(Manager), Selami Korkmaz(Designer)...</author>
        <long-description>Mandalina is an unmanned aerial vehicle produced by the IDEALAB company...</long-description>
        <aircraft-version>1.0</aircraft-version>
        <!--Engine and fuel information-->
        <fligh-model>yasim</fligh-model>
        <aero>aircraft</aero>
        <fuel-fraction>1.0</fuel-fraction>
        <!--Engine and fuel information-->
        <sound>
            <audible>False</audible>
            <path>Aircraft/Deneme/Sounds/mandalina-sound.xml</path>
            </sound>
        <panel>
            <visibility archive="n">False</visibility>
            </panel>
        <model>
            <path archive="y">Aircraft/Deneme/Models/Deneme.xml</path>
            </model>
        <previews>
            <preview>
                <type>exterior</type>
                <path>Previews/turkey.png</path>
                <splash>true</splash>
                </preview>
            </previews>
        <tags>
            <tag>high-wing</tag>
            <tag>retractable-gear</tag>
            <tag>single-engine</tag>
            </tags>
        <rating>
            <FDM>2</FDM>
            <systems>1</systems>
            <cockpit>0</cockpit>
            <model>2</model>
            </rating>
        <help>
            <title>Mandalina, Version 1.0 (IL-170)</title>
            <line>Cruise speed: 0.2 Mach</line>
            <line>Never-exceed (Vne): 0.6 Mach</line>
            <line />
            </help>
        <view>
            <internal archive="y">true</internal>
            <config>
                <x-offset-m archive="y">0.0</x-offset-m>
                <y-offset-m archive="y">-0.3</y-offset-m>
                <z-offset-m archive="y">0.9</z-offset-m>
                <pitch-offset-deg>-8</pitch-offset-deg>
                </config>
            </view>
        <consumables>
            <fuel>
                <tank n="0">
                    <name>6L-Left Tank</name>
                    <capacity unit="LBS">13.2</capacity>
                    <selected type="bool">true</selected>
                    </tank>
                <tank n="1">
                    <name>6L-Right Tank</name>
                    <capacity unit="LBS">13.2</capacity>
                    <selected type="bool">true</selected>
                    </tank>
                </fuel>
            </consumables>
        </sim>
    <engines>
        <engine>
            <rpm>500</rpm>
            </engine>
        </engines>
    <fdm>
        <flight>
            <elevator-trim>0.0</elevator-trim>
            <elevator-trim>0.0</elevator-trim>
            <rudder-trim>0.0</rudder-trim>
            </flight>
        </fdm>
    <controls>
        <engines>
            <active-engine>0</active-engine>
            <running>true</running>
            </engines>
        </controls>
    </PropertyList>
